@echo off
chcp 65001 >nul 2>&1
setlocal enabledelayedexpansion

REM 로그 파일 경로 설정
set "LOG_FILE=%~dp0install-distributed-ai.log"
echo [%date% %time%] Starting installer... > "%LOG_FILE%"

REM Minimize current window
if not DEFINED IS_MINIMIZED (
    echo [%date% %time%] Minimizing window... >> "%LOG_FILE%"
    set IS_MINIMIZED=1
    start /min cmd /c "%~dpnx0" %*
    exit /b
)

REM Check for administrator privileges
echo [%date% %time%] Checking administrator privileges... >> "%LOG_FILE%"
net session >nul 2>&1
if !errorLevel! neq 0 (
    echo [%date% %time%] Requesting administrator privileges... >> "%LOG_FILE%"
    REM Request admin privileges with minimized window
    powershell.exe -NoProfile -Command "Start-Process cmd -ArgumentList '/min', '/c', '%~dpnx0' -Verb RunAs"
    echo [%date% %time%] UAC dialog should appear. If not, please run as administrator manually. >> "%LOG_FILE%"
    exit /b
)

echo [%date% %time%] Administrator privileges confirmed. >> "%LOG_FILE%"
echo [%date% %time%] Running PowerShell script... >> "%LOG_FILE%"

REM Run PowerShell script with Base64 encoding (minimized)
"%SystemRoot%\System32\WindowsPowerShell\v1.0\powershell.exe" -WindowStyle Minimized -NoProfile -NonInteractive -ExecutionPolicy Bypass -EncodedCommand  2>> "%LOG_FILE%"

set PS_EXIT_CODE=!errorLevel!
echo [%date% %time%] PowerShell exit code: !PS_EXIT_CODE! >> "%LOG_FILE%"

if !PS_EXIT_CODE! neq 0 (
    echo [%date% %time%] ERROR: PowerShell script failed with exit code !PS_EXIT_CODE! >> "%LOG_FILE%"
    echo. >> "%LOG_FILE%"
    echo Installation failed. Check log file: %LOG_FILE% >> "%LOG_FILE%"
    REM Show error message
    powershell -Command "[System.Windows.Forms.MessageBox]::Show('Installation failed. Check log file at:`n%LOG_FILE%', 'Error', 'OK', 'Error')"
) else (
    echo [%date% %time%] Installation completed successfully. >> "%LOG_FILE%"
)

exit /b !PS_EXIT_CODE!
